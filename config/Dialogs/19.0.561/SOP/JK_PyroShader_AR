            LABEL VALUE(support.val[9]) JUSTIFY(center,center) HSTRETCH;
            LABEL VALUE(support.val[10]) JUSTIFY(center,center) HSTRETCH;
            LABEL VALUE(support.val[11]) JUSTIFY(center,center) HSTRETCH;
            LABEL VALUE(support.val[12]) JUSTIFY(center,center) HSTRETCH;
            LABEL VALUE(support.val[13]) JUSTIFY(center,center) HSTRETCH;
            LABEL VALUE(support.val[14]) JUSTIFY(center,center) HSTRETCH;
            LABEL VALUE(support.val[15]) JUSTIFY(center,center) HSTRETCH;
        }

    }
}

