dirs.parm =
{
    LAYOUT(horizontal) HSTRETCH JUSTIFY(left, center)

    PARM_LABEL_SIMPLE("Filesystem Folders", "dirs") WIDTH(1.5);
    { WIDTH(0.1) }

    ROW
    {
	HSTRETCH(0.5) JUSTIFY(left, center) VALUE(dirs.val)

        dirs.field = PARM_FIELD (integer, "dirs")
				VALUE(dirs.val) HSTRETCH(0.5)
				 PREF_WIDTH(0.5);

        ROW
        {
	     LOOK(lightline) VMARGIN(1p)

   	     INCREMENT_ICONBUTTON VALUE(dirs.more.val)	
	    	HELP("Add Another Instance") SIZE(0.2, 0.24);
	     DECREMENT_ICONBUTTON VALUE(dirs.less.val)	
	        HELP("Remove Last Instance") SIZE(0.2, 0.24);

	}

	ACTION_BUTTON "Clear" VALUE(dirs.clear.val);
    }

    { HSTRETCH }
}

dirs.multilist = 
{
    LAYOUT(vertical) HSTRETCH MARGIN(0.05)
}

dirs.gad =
{
    LAYOUT(vertical) HSTRETCH
    HMARGIN(0.18, 0.1) NO_MARGIN_SPACING_SCALE

    SWITCHER tight
    {
	LAYOUT(vertical) HSTRETCH

	VALUE(dirs.multilist_hide_buttons.val)
	{
	    LAYOUT(vertical) HSTRETCH
	    dirs.parm;
	    {LOOK(none) HEIGHT(0.02)}
	}
	{
	}
    }
    dirs.multilist;
}

